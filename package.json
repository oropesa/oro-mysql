{
  "name": "oro-mysql",
  "version": "2.1.0",
  "description": "OroMysql Class is a wrapper of npm-mysql2 to use async/await",
  "type": "module",
  "main": "./.cjs",
  "module": "./.js",
  "types": "./.d.ts",
  "exports": {
    ".": {
      "require": "./.cjs",
      "import": "./.js",
      "types": "./.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:open": "node coverage.open.js",
    "coverage-open": "jest --coverage & node coverage.open.js",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts",
    "watch": "nodemon"
  },
  "keywords": [
    "mysql",
    "promise",
    "async/await",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-mysql.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-mysql/issues"
  },
  "homepage": "https://github.com/oropesa/oro-mysql#readme",
  "dependencies": {
    "mysql2": "^3.10.2",
    "oro-functions-client": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "oro-functions": "^2.3.1",
    "oro-timer": "^2.1.1",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
