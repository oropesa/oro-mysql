{
  "name": "oro-mysql",
  "version": "2.0.0",
  "description": "OroMysql Class is a wrapper of npm-mysql2 to use async/await",
  "type": "module",
  "main": "./.cjs",
  "module": "./.js",
  "types": "./.d.ts",
  "exports": {
    ".": {
      "require": "./.cjs",
      "import": "./.js",
      "types": "./.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:open": "node coverage.open.js",
    "coverage-open": "jest --coverage & node coverage.open.js",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts",
    "watch": "nodemon"
  },
  "keywords": [
    "mysql",
    "promise",
    "async/await",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-mysql.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-mysql/issues"
  },
  "homepage": "https://github.com/oropesa/oro-mysql#readme",
  "dependencies": {
    "mysql2": "^3.9.7",
    "oro-functions-client": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^28.4.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "oro-functions": "^2.1.1",
    "oro-timer": "^2.0.6",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
